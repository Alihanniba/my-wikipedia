{
    "title": "My-WikiPedia",
    "author": "alihanniba",
    "generator": "site",
    "description": "记录Gitbook的配置和一些插件的使用",
    "language": "zh-hans",
    "links": {
        "sidebar": {
            "alihanniba": "https://www.alihanniba.com"
        }
    },
    "plugins": [
        "disqus",
        "-search",
        "search-pro",
        "advanced-emoji",
        "github",
        "ace",
        "emphasize",
        "katex",
        "anchors",
        "include-codeblock",
        "mermaid",
        "sectionx",
        "expandable-chapters",
        "codeblock-filename",
        "baidu",
        "splitter",
        "fontsettings",
        "toggle-chapters",
        "sectionx-strong",
        "customtheme",
        "page-footer"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "gitbookuse"
        },
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord": [
                "Gitbook Use"
            ]
        },
        "github": {
            "url": "https://github.com/alihanniba/"
        },
        "baidu": {
            "token": "ff100361cdce95dd4c8fb96b4009f7bc"
        },
        "fontsettings": {
            "theme": "sepia",
            "family": "serif",
            "size": 2
        },
        "customtheme": {
            "js": [
                "isset/index.js",
                "isset/gitbook.js"
            ]
        },
        "page-footer": {
            "description": "modified at",
            "author": "alihanniba",
            "wisdom": "GO TO BE A FULL STACK   GO TO BE A ARCHITECT",
            "format": "YYYY-MM-DD HH:mm:ss",
            "copyright": "Copyright © alihanniba",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "qrcode": true,
            "baseUri": "https://alihanniba.gitbooks.io/my-wikipedia/content/"
        }
    }
}